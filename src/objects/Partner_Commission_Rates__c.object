    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>This custom object allow users to create records to define stand commission rates that are based on a Type of Commission and related to Products (InvoiceIT)</description>
    <enableActivities>true</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ReadWrite</externalSharingModel>
    <fields>
        <fullName>Commission_Type__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Select the type of order the commission is based on.</inlineHelpText>
        <label>Commission Type</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Rental</fullName>
                    <default>false</default>
                    <label>Rental</label>
                </value>
                <value>
                    <fullName>Sale</fullName>
                    <default>false</default>
                    <label>Sale</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>External_Id__c</fullName>
        <caseSensitive>false</caseSensitive>
        <externalId>true</externalId>
        <label>External Id</label>
        <length>255</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Partner_Agent_Account__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <inlineHelpText>Select the Partner / Sales Agent</inlineHelpText>
        <label>Partner / Agent Account</label>
        <lookupFilter>
            <active>true</active>
            <filterItems>
                <field>Account.RecordTypeId</field>
                <operation>equals</operation>
                <value>KV Partner</value>
            </filterItems>
            <infoMessage>This currently looks up to KV Partners. The value entered does not exist or does not match filter criteria. Remove filter criteria to search all accounts.</infoMessage>
            <isOptional>true</isOptional>
        </lookupFilter>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Partner Commission Rates</relationshipLabel>
        <relationshipName>Partner_Commission_Rates</relationshipName>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Product__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <inlineHelpText>This looks up to the InvoiceIT Product</inlineHelpText>
        <label>Product</label>
        <referenceTo>Product__c</referenceTo>
        <relationshipLabel>Partner Commission Rates</relationshipLabel>
        <relationshipName>Partner_Commission_Rates</relationshipName>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Standard_Comm__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Enter the standard commission for this Partner Agent for the selected Product for all subsequent years after year 1</inlineHelpText>
        <label>Standard Comm (after Yr1)</label>
        <precision>7</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Year1_Commission__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Commission for Year 1. This is specific to Rentals and Subscriptions</inlineHelpText>
        <label>Year1 Commission</label>
        <precision>4</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <label>Partner Commission Rates</label>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
        <displayFormat>PCR-{0000}</displayFormat>
        <label>Partner Commission Rates Name</label>
