    <validationRules>
        <fullName>RMA_off_Hold_Date_GreaterThan_Today</fullName>
        <active>true</active>
        <description>RMA Hold Date cannot be greater than today</description>
        <errorConditionFormula>Date_Unit_off_Hold__c &gt;  TODAY()</errorConditionFormula>
        <errorMessage>You cannot set the RMA off Hold Date to be greater than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RMA_off_Hold_Date_Less_Than_On_Hold_Date</fullName>
        <active>true</active>
        <description>RMA Hold Date cannot be Less than the On Hold Date</description>
        <errorConditionFormula>Date_Unit_off_Hold__c &lt; Date_Unit_on_Hold__c</errorConditionFormula>
        <errorMessage>You cannot set the RMA off Hold Date to be less than the Date On Hold</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RMA_on_Hold</fullName>
        <active>true</active>
        <description>RMA on Hold cannot have an RMA Off Hold Date</description>
        <errorConditionFormula>And(ISPICKVAL(RMA_Status__c, &quot;on Hold&quot;), Date_Unit_off_Hold__c  &gt;  DATE(2015,01,01) )</errorConditionFormula>
        <errorMessage>You cannot set the RMA on Hold if there is an RMA off Hold Date</errorMessage>
    </validationRules>
